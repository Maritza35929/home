<p id="notice"><%= notice %></p>

<h1>Tecnicos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Telefono</th>
      <th>Profesion u oficio</th>
      <th>Servicios ofrecidos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tecnicos.each do |tecnico| %>
      <tr>
        <td><%= tecnico.nombre %></td>
        <td><%= tecnico.telefono %></td>
        <td><%= tecnico.profesion_u_oficio %></td>
        <td><%= tecnico.servicios_ofrecidos %></td>
        <td><%= link_to 'Show', tecnico %></td>
        <td><%= link_to 'Edit', edit_tecnico_path(tecnico) %></td>
        <td><%= link_to 'Destroy', tecnico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tecnico', new_tecnico_path %>
